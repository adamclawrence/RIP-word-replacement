{
  "manifest_version": 2,

  "name": "RIP-Word Replacement",
  "description": "Wow we are just starting this.",
  "version": "0.5",

  "icons": {
    "16": "Icon16.png",
    "48": "Icon48.png",
    "128":"Icon128.png"
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["rip.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}